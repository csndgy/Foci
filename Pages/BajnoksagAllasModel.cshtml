@page
@model focisokadik.Pages.BajnoksagAllasModelModel
@{
    ViewData["Title"] = "Bajnokság állása";
}
<h1>helo</h1>
<table class="table">
<thead>
    <tr>
        <th>Helyezés</th>
        <th>Csapat Neve</th>
        <th>Játszott Mérkőzés</th>
        <th>Gy</th>
        <th>D</th>
        <th>V</th>
        <th>Pontszám</th>
        <th>Lőtt gól</th>
        <th>Kapott Gól</th>
    </tr>
</thead>
<tbody>
    @for (int i = 0; i < Model.csapatokEredmenyei.Count(); i++)
    {
        <tr>
            <td>@(i+1)</td>
            <td>@Model.csapatokEredmenyei[i].CsapatNev</td>
            <td></td>
            <td>@Model.csapatokEredmenyei[i].GyozelmekSzama</td>
        </tr>
    }
</tbody>
</table>